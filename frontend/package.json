{
  "version": "1.0.0",
  "name": "frontend",
  "description": "The frontend project for a ASP.NET Core Web API that measures air temperature through a Arduino IoT device.",
  "license": "MIT",
  "author": "Burhan Mohammad Sarfraz",
  "private": true,
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "development": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 edge version"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "dev": "vite",
    "format": "prettier --write \"*.{ts,tsx,css,html,json}\"",
    "format:check": "prettier --check \"*.{ts,tsx,css,html,json}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "nswag": "nswag run nswag.json /runTime:Net80 --write src/**/api.generated.ts",
    "preview": "vite preview",
    "prepush": "npm run format && npm run lint",
    "start": "vite"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.37.1",
    "@tanstack/react-query-devtools": "^5.37.1",
    "nswag": "^14.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
