{
  "version": "0.0.25",
  "name": "frontend",
  "author": "Burhan Mohammad Sarfraz <burhan.mohammad.sarfraz@gmail.com>",
  "description": "The frontend application for a ASP.NET Core Web API that measures temperature and air humidity through a Arduino IoT device.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hvaheterdu/arduino-thermometer"
  },
  "private": true,
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "development": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 edge version"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "dev": "vite",
    "format": "prettier --write \"*.{ts,tsx,css,html,json}\"",
    "format:check": "prettier --check \"*.{ts,tsx,css,html,json}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint-staged": "lint-staged",
    "nswag": "nswag run nswag.json /runTime:Net80 --write src/**/api.generated.ts",
    "preview": "vite preview",
    "prepare": "cd .. && husky frontend/.husky",
    "start": "vite",
    "version-bump": "sh scripts/version_bump.sh"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.39.0",
    "@tanstack/react-query-devtools": "^5.39.0",
    "nswag": "^14.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import-x": "^0.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
